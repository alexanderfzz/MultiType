<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="grooop.mapper.UtilMapper">
    <update id="resetUserAutoIncrement" parameterType="int">
        alter table user auto_increment=#{value}
    </update>

    <update id="resetQuoteAutoIncrement" parameterType="int">
        alter table quote auto_increment=#{value}
    </update>

    <update id="resetRecordAutoIncrement" parameterType="int">
        alter table record auto_increment=#{value}
    </update>

</mapper>